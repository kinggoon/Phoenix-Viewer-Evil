<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Netzwerk &amp; Verzeichnisse" name="network">
	<text name="text_box">
		Maximale Bandbreite:
	</text>
	<text name="text_box2">
		kbit/s (Kilobit pro Sekunde)
	</text>
	<text name="cache_size_label_l">
    Cache Verzeichnis Größe:
  </text>
  <slider left_delta="133" name="cache_size" />
	<text name="text_box5">
		MB
	</text>
  <button label="Cache löschen" name="clear_cache"/>
  <button label="Inventar Cache löschen" name="clear_inv_cache"/>
	<text name="cache_location_label">
    Festplatten Cache Pfad:
  </text>
  <button label="Öffnen" tool_tip="Dieses Verzeichnis öffnen" width="75" name="open_cache_loc"/>
  <button label="Festlegen" tool_tip="Neues xxx Verzeichnis festlegen" left_delta="55" width="75" label_selected="Festlegen" name="set_cache"/>
	<button label="Zurücksetzen" tool_tip="Standard Cache Verzeichnis verwenden" left_delta="55" label_selected="Festlegen" name="reset_cache"/>
  <text name="sound_cache_location_label">
    Sound Cache Pfad:
  </text>
  <button label="Öffnen" tool_tip="Dieses Verzeichnis öffnen" width="75" name="open_soundcache_loc"/>
  <button label="Festlegen" tool_tip="Neues xxx Verzeichnis festlegen" left_delta="55" width="75" label_selected="Festlegen" name="set_sound_cache"/>
  <button label="Zurücksetzen" tool_tip="Standard Cache Verzeichnis verwenden" left_delta="55" label_selected="Festlegen" name="reset_sound_cache"/>
  <text name="chat_location_label">
    Chat Log Pfad:
  </text>
  <button label="Öffnen" tool_tip="Dieses Verzeichnis öffnen" width="75" name="open_chat_logs_loc"/>
  <button label="Festlegen" tool_tip="Neues xxx Verzeichnis festlegen" left_delta="55" width="75" label_selected="Festlegen" name="set_chat_logs"/>
  <button label="Zurücksetzen" tool_tip="Standard Cache Verzeichnis verwenden" left_delta="55" label_selected="Festlegen" name="reset_chat_logs"/>
  <text name="crash_location_label">
    Crash Log Pfad:
  </text>
  <button label="Öffnen" tool_tip="Dieses Verzeichnis öffnen" width="75" name="open_crash_logs_loc"/>
  <text name="cannot_change_crash_label">
    (Nicht änderbar)
  </text>
  <text name="user_location_label">
    User Einstellungen Pfad:
  </text>
  <button label="Öffnen" tool_tip="Dieses Verzeichnis öffnen" width="75" name="open_user_loc"/>
  <text name="cannot_change_user_label">
    (Nicht änderbar)
  </text>
  <check_box label="Benutzerdefinierter Port" name="connection_port_enabled"/>
	<spinner label="Port-Nummer:" width="150" name="connection_port"/>
  <check_box label="Socks 5 Proxy verwenden" name="socks5_proxy_enabled"/>
  <text name="socks5_type_label">
    Proxy Typ:
  </text>
  <radio_group left="35" name="socks5_http_proxy_type">
    <radio_item name="None" tool_tip="Keinen Http Proxy">
      Keinen
    </radio_item>
    <radio_item name="Socks" tool_tip="Socks für Http-Proxy verwenden">
      Socks
    </radio_item>
    <radio_item height="20" bottom="0" left="250" name="Web" width="50"
		  tool_tip="WebProxy für Http-Proxy verwenden">
      Web
    </radio_item>
  </radio_group>
  <text width="135" name="socks5_auth_label">
    Socks5 Authentifizierung:
  </text>
  <radio_group name="socks5_auth">
    <radio_item bottom="0" height="20" left_delta="85" name="None" width="50"
   tool_tip="Keinen Authentifizierung">
      Keinen
    </radio_item>
    <radio_item bottom="0" height="20" left_delta="40" name="UserPass" width="50"
   tool_tip="Username / Passwordt Authentifizierung">
      Username/Passwort
    </radio_item>
  </radio_group>
  <text name="socks5_password_label">
    Passwort:
  </text>
</panel>
